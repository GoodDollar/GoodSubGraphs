{
  "name": "good-subgraphs-fuse",
  "license": "UNLICENSED",
  "scripts": {
    "gv": "graph version",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ GoodDollar/GoodSubGraphsFuse",
    "create-local": "graph create --node http://localhost:8020/ GoodDollar/GoodSubGraphsFuse",
    "remove-local": "graph remove --node http://localhost:8020/ GoodDollar/GoodSubGraphsFuse",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 GoodDollar/GoodSubGraphsFuse",
    "dev": "npm run codegen && npm run deploy-local",
    "dev:nodemon": "node_modules/nodemon/bin/nodemon.js --exec 'npm run dev' --ignore build/ --ignore generated/ --ext 'ts,json,graphql,yaml'",
    "contract-releases": "node scripts/get-contract-releases.js"
  },
  "dependencies": {
    "@gooddollar/goodcontracts": "^2.2.5",
    "@graphprotocol/graph-cli": "^0.19.0",
    "@graphprotocol/graph-ts": "^0.19.0"
  },
  "version": "0.0.0",
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}